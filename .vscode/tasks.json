{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build Angular",
            "type": "shell",
            "group": "build",
            "command": "ng build",
            "problemMatcher": [
                "$tsc"
            ]
        },
        {
            "label": "build Angular Production",
            "type": "shell",
            "group": "build",
            "command": "ng build --prod",
            "problemMatcher": [
                "$tsc"
            ]
        }
    ]
}